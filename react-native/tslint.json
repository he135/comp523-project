{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
		"tslint-react"
    ],
    "jsRules": {},
	"rules": {
        "no-any": true,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "object-literal-sort-keys": false,
        "jsx-no-multiline-js": false,
        "trailing-comma": false
	},
    "rulesDirectory": []
}
